<%var index = 0;var currentUser;messages.forEach(function(message) { index++;if(!currentUser || message.sender !== currentUser){currentUser = message.sender;index = 0;}if(index >= 5) index = 0;%><div class="msg<%=index == 0 ? ' msgA' : ''%>"><%if(index == 0){%><div class="header"><img width="45px" height="45px" src="<%= users[ message.sender - 1 ].pfp != 0 ? 'images/profile_pictures/'+currentUser+'.png' : 'images/pfp.png'; %>"><h1><%=users[ message.sender - 1]['pseudo']%></h1></div><%}%><p class="msgContent"><%= message.content; %></p></div><% });%>